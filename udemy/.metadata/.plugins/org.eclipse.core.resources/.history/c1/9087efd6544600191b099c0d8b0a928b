package Secao04Exercicios;

import java.util.Scanner;

public class e8 {

	public static void main(String[] args) {
			int n1, n2, nota1, nota2;
				Scanner teclado = new Scanner (System.in);
				
				System.out.println("digite a nota 1");
				n1 = teclado.nextInt();
				if (n1 < 0) {
					System.out.println("nota invalidade");
					System.exit(n1);
				}
					

				System.out.println("digite a nota 2");
				n2 = teclado.nextInt();
				
								
				if (n1 < 0) {
					if (n1 > 10);
					System.out.println("o nota é invalida");
					System.exit(0);
				} else if (n2 < 0) {
					if (n2 > 01)
						System.out.println("o nota é invalida");
						System.exit(0);
				}
					
		teclado.close();
	
	}
				
}

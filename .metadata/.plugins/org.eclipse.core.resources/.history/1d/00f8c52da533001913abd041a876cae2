package Exercicios;

public class Pessoa2 {
	
	
	String pessoa_nome[] = new String[2];
	String nome;
	
	public Pessoa2(String nome) {
		for(int i=0; i <pessoa_nome.length; i++) {
			this.pessoa_nome[i]= nome;
		}
	}
	
	void imprime() {
				
		for(int i=0; i <pessoa_nome.length-1; i++) {
			System.out.println(pessoa_nome[i]);
		}
	}
	
}	

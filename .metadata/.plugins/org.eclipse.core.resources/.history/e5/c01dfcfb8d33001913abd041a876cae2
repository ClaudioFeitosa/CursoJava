package Exercicios;

public class P2 {
	
	//String nome;
	String pessoa_nome[] = new String[2];
	
	public P2(String nome) {
		for(int i=0; i <pessoa_nome.length; i++) {
			this.pessoa_nome[i]= nome;
		}
	}
	
	void imprime() {
		for(int j=0; j <pessoa_nome.length; j++) {
			System.out.println("nome " + pessoa_nome[j]);
		}
	}
	
}	
